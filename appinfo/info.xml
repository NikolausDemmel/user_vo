<?xml version="1.0" encoding="UTF-8"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
	  xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>user_vo</id>
    <name>VereinOnline User Authentication</name>
    <summary>User authentication plugin for VereinOnline</summary>
	<description><![CDATA[Use VereinOnline User Authentication to give VereinOnline users access to your Nextcloud instance.

The [documentation](https://github.com/NikolausDemmel/user_vo) describes how this plugin is configured.
    ]]></description>
    <licence>AGPL</licence>
    <author>Nikolaus Demmel</author>
    <version>0.2.0</version>
	<namespace>UserVO</namespace>
	<types>
		<prelogin/>
		<authentication/>
	</types>
	<documentation>
		<admin>https://github.com/NikolausDemmel/user_vo</admin>
	</documentation>
	<category>tools</category>
	<category>integration</category>
	<website>https://github.com/NikolausDemmel/user_vo</website>
	<bugs>https://github.com/NikolausDemmel/user_vo/issues</bugs>
	<repository type="git">https://github.com/NikolausDemmel/user_vo.git</repository>
	<dependencies>
		<nextcloud min-version="24" max-version="31" />
	</dependencies>
	<settings>
    <admin>OCA\UserVO\Settings\UserVOAdminSettings</admin>
    <admin-section>OCA\UserVO\Sections\UserVOAdminSection</admin-section>
</settings>
</info>
